        <td>
            @Html.DisplayFor(modelItem => item.DateModified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedBy)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.VehicleCostID }) |
            @Html.ActionLink("Details", "Details", new { id = item.VehicleCostID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.VehicleCostID })
        </td>
    </tr>
}

</table>
