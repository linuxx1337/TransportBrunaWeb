@model IEnumerable<TransportBrunaWeb.Models.VehicleCosts>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Costs.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costs.CostTypes.Name)
